{
    "name": "Vcheck",
    "description" : "视觉还原辅助工具",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_title": "Vcheck"
      },
      "icons": {
        "16": "icon/icon.png",
        "48": "icon/icon.png",
        "128": "icon/icon.png"
      },
        "permissions": [
        "tabs",
        "tabCapture",
        "storage"
    ],
    "content_security_policy": "script-src 'self'; object-src 'self'"
}