{
    "name": "UI Restore",
    "description" : "视觉还原工具",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_title": "VCheck"
      },
      "icons": {
        "16": "icon/icon-16.png",
        "48": "icon/icon-48.png",
        "128": "icon/icon-128.png"
      },
        "permissions": [
        "tabs",
        "tabCapture",
        "storage"
    ],
    "content_security_policy": "script-src 'self' http://* https://*; object-src 'self'"
}